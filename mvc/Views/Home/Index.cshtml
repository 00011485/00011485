@model List<User>

@{
    ViewData["Title"] = "User List";
}

<div class="d-flex justify-content-between align-items-center mb-3">
    <h1>User List</h1>
    <button class="btn btn-success">Create User</button>
</div>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Username</th>
            <!-- Add other headers as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>@user.Username</td>
            </tr>
        }
    </tbody>
</table>
